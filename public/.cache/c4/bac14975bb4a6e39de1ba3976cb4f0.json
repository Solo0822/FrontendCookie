kie-category {\r\n    margin-bottom: 10px;\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n}\r\n\r\n.cookie-category label {\r\n    font-size: 14px;\r\n    color: #ddd;\r\n    margin-bottom: 0;\r\n}\r\n\r\n.cookie-category input {\r\n    transform: scale(1.2);\r\n}\r\n\r\n.cookie-buttons {\r\n    display: flex;\r\n    gap: 10px;\r\n}\r\n\r\n.cookie-buttons button {\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-size: 14px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s;\r\n}\r\n\r\n.cookie-buttons .accept-button {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n}\r\n\r\n.cookie-buttons .reject-button {\r\n    background-color: #f44336;\r\n    color: white;\r\n}\r\n\r\n.cookie-buttons .customize-button {\r\n    background-color: #2196F3;\r\n    color: white;\r\n}\r\n\r\n.cookie-buttons button:hover {\r\n    opacity: 0.8;\r\n}\r\n\r\n@keyframes fadeIn {\r\n    0% {\r\n        opacity: 0;\r\n        top: -50px;\r\n    }\r\n    100% {\r\n        opacity: 1;\r\n        top: 0;\r\n    }\r\n}\r\n\r\n/* Login Page Styles */\r\n.login-container {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    width: 100%;\r\n    max-width: 400px;\r\n    background-color: rgba(0, 0, 0, 0.6);\r\n    border-radius: 10px;\r\n    padding: 30px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.login-card {\r\n    width: 100%;\r\n}\r\n\r\n.card-header h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    margin-bottom: 20px;\r\n    color: #fff;\r\n}\r\n\r\n.input-group {\r\n    margin-bottom: 15px;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.input-group label {\r\n    font-size: 14px;\r\n    color: #ccc;\r\n    margin-bottom: 5px;\r\n}\r\n\r\n.input-group input {\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    background-color: #333;\r\n    color: white;\r\n    outline: none;\r\n}\r\n\r\n.input-group input::placeholder {\r\n    color: #bbb;\r\n}\r\n\r\n.login-button {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    padding: 12px;\r\n    width: 100%;\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n    margin-bottom: 10px;\r\n    transition: background-color 0.3s;\r\n}\r\n\r\n.login-button:hover {\r\n    background-color: #45a049;\r\n}\r\n\r\n.forgot-password {\r\n    color: #bbb;\r\n    text-align: center;\r\n    display: block;\r\n    margin-top: 10px;\r\n    text-decoration: none;\r\n}\r\n\r\n.forgot-password:hover {\r\n    color: #fff;\r\n}\r\n\r\n.social-login {\r\n    text-align: center;\r\n    margin-top: 20px;\r\n}\r\n\r\n.social-login p {\r\n    font-size: 14px;\r\n    color: #ccc;\r\n    margin-bottom: 15px;\r\n}\r\n\r\n.social-button {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    padding: 12px 20px;\r\n    font-size: 16px;\r\n    background-color: transparent;\r\n    border: 1px solid #ccc;\r\n    color: #ccc;\r\n    cursor: pointer;\r\n    width: 100%;\r\n    border-radius: 5px;\r\n    margin-bottom: 10px;\r\n    transition: background-color 0.3s;\r\n}\r\n\r\n.social-button:hover {\r\n    background-color: rgba(255, 255, 255, 0.1);\r\n}\r\n\r\n.social-button img {\r\n    width: 20px;\r\n    margin-right: 10px;\r\n}\r\n\r\n/* Responsive Design */\r\n@media (max-width: 768px) {\r\n    .login-container {\r\n        width: 90%;\r\n    }\r\n\r\n    .cookie-banner {\r\n        width: 90%;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c8c39750204aad2cc803ea9cf8ff7f29","cacheData":{"env":{}}}